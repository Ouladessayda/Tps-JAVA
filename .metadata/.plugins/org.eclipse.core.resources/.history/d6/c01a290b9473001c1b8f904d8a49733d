package cigma.pfe.models;

import javax.persistence.Entity;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
@AllArgsConstructor
@Entity(name = "TClients")
public class Client {
	
	 public long Id;
	 public String Name;
	 
	 
	 public Client(long Id , String Name) {
		 
		 this.Id=Id;
		 this.Name=Name;
	 }
	 
	 public Client() {
		 
		 
	 }
	

}
