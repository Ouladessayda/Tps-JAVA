package ma.education.tp6.annotations;

public class TestReflectionAnnotation {

	public static void main(String[] args) {
		

Class Affichage=Calculatrice.class;

Programmer P1 = (Programmer) Affichage.getDeclaredAnnotation(Programmer.class);

// Affichage des valeurs de l'annotation de la classe m√®re
System.out.println(P1.id()+":"+P1.name());



 Affichage=CalculatriceMath.class;

 Programmer P2 = (Programmer) Affichage.getAnnotation(Programmer.class);

//Affichage des valeurs de l'annotation de la classe fille
System.out.println(P2.id()+":"+P2.name());

	}

}
